data(iris)
mytable <- table(iris$Species)
# paste(multiple R objects, sep=" ") so we put the name and the number
print(mytable)
lbls <- paste(names(mytable), "\n", 50, sep="")
pie(mytable, labels = lbls,
main="Pie Chart of Species\n (with sample sizes)")
# Histogram
par(mfrow=c(2,2))
